{{!--
Module for enqueueing assets
@author zkreations
--}}
<b:includable id='m:enqueue'>
  {{!-- Main CSS --}}
  <style>/*<![CDATA[*/
  {{#if development}}
    {{asset '/dist/css/main.css'}}
  {{else}}
    {{asset '/dist/css/main.min.css'}}
  {{/if}}
  /*]]>*/</style>
  {{!-- Post and Page CSS --}}
  <b:if cond='data:view.isSingleItem'>
    <style>/*<![CDATA[*/
    {{#if development}}
      {{asset '/dist/css/typography.css'}}
    {{else}}
      {{asset '/dist/css/typography.min.css'}}
    {{/if}}
    /*]]>*/</style>
  </b:if>
  {{!-- Main Js --}}
  {{#unless development}}
  <script defer src='https://cdn.jsdelivr.net/gh/zkreations/basico@2/dist/js/main.min.js'></script>
  {{/unless}}
</b:includable>
