{{!--
General Settings
--}}
[image] Url of default avatar.
<Variable name="c.avatar" value="https://lh3.googleusercontent.com/a/default-user"/>

[image] HD Favicon URL.
<Variable name="c.favicon"/>

[string] Formspree.io form ID.
<Variable name="c.formspree"/>

[image] Url of default image.
<Variable name="c.image"/>

[string] Image parameters: https://zkreations.com/image-params
<Variable name="c.imageParams" value="rw-l80-e30"/>

[image] Url of default Open Graph image.
<Variable name="c.ogImage"/>

[url] Default author profile URL.
<Variable name="c.profileUrl" value="https://www.blogger.com/"/>

[string] Ui color scheme. (dark, light)
<Variable name="c.uiScheme" value="light"/>

[boolean] Enable ui color scheme toggle.
<Variable name="c.uiToggle" value="true"/>
